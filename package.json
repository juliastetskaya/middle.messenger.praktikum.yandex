{
  "name": "middle.messenger.praktikum.yandex",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "webpack-dev-server",
    "build": "webpack",
    "start": "npm run build && node ./server.js",
    "lint": "npx stylelint '**/*.css' && eslint .",
    "check": "tsc --noEmit",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juliastetskaya/middle.messenger.praktikum.yandex.git"
  },
  "author": "Julia Stetskaya <juliastetskaya@gmail.com>",
  "engines": {
    "node": ">=12.0.0"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "alias": {
    "core": "./src/core",
    "components": "./src/components",
    "blocks": "./src/blocks",
    "data": "./src/data",
    "static": "./src/static",
    "utils": "./src/utils",
    "API": "./src/API",
    "services": "./src/services",
    "HOC": "./src/HOC"
  },
  "dependencies": {
    "css-loader": "^6.7.3",
    "express": "4.18.1",
    "handlebars": "^4.7.7",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "mini-css-extract-plugin": "^2.7.2",
    "ts-loader": "^9.4.2",
    "uuid": "^9.0.0",
    "webpack-dev-server": "^4.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/express": "^4.17.14",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.11.9",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "eslint": "8.24.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "jest": "^27.5.1",
    "postcss-import": "15.0.0",
    "stylelint": "14.13.0",
    "stylelint-config-standard": "28.0.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|svg|png)$": "identity-obj-proxy",
      "^core(.*)$": "<rootDir>/src/core$1",
      "^components(.*)$": "<rootDir>/src/components$1",
      "^blocks(.*)$": "<rootDir>/src/blocks$1",
      "^data(.*)$": "<rootDir>/src/data$1",
      "^static(.*)$": "<rootDir>/src/static$1",
      "^utils(.*)$": "<rootDir>/src/utils$1",
      "^API(.*)$": "<rootDir>/src/API$1",
      "^services(.*)$": "<rootDir>/src/services$1",
      "^HOC(.*)$": "<rootDir>/src/HOC$1"
    }
  }
}
